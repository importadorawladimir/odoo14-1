<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_document_type_electronic_code_form" model="ir.ui.view">
        <field name="name">view.document.type.electronic_code.form</field>
        <field name="model">l10n_latam.document.type</field>
        <field name="inherit_id" ref="l10n_latam_invoice_document.view_document_type_form"/>
        <field name="arch" type="xml">
             <xpath expr="//field[@name='code']" position="after">
                <!-- Add your fields or attributes here -->
                 <field name="electronic_code"/>
            </xpath>
        </field>
    </record>


    <record id="view_document_type_electronic_code_tree" model="ir.ui.view">
        <field name="name">view.document.type.electronic_code.tree</field>
        <field name="model">l10n_latam.document.type</field>
        <field name="inherit_id" ref="l10n_latam_invoice_document.view_document_type_tree"/>
        <field name="arch" type="xml">
             <xpath expr="//field[@name='code']" position="after">
                <!-- Add your fields or attributes here -->
                 <field name="electronic_code"/>
            </xpath>
        </field>
    </record>


    <record id="view_l10n_latam_identification_type_eletronic_tree" model="ir.ui.view">
        <field name="name">l10n_latam.identification.type.eletronic.tree</field>
        <field name="model">l10n_latam.identification.type</field>
        <field name="inherit_id" ref="l10n_latam_base.view_l10n_latam_identification_type_tree"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='name']" position="after">
                <field name="electronic_code"/>
            </xpath>

        </field>
    </record>

</odoo>
