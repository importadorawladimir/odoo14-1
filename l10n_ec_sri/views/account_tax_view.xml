<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="account_tax_form_electronic_view" model="ir.ui.view">
		<field name="name">account.tax.electronic.form</field>
		<field name="model">account.tax</field>
		<field name="type">form</field>
		<field name="inherit_id" ref="account.view_tax_form"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='type_tax_use']" position="after">
				<field name="l10n_ec_electronic_code" groups="base.group_no_one"/>
			</xpath>
		</field>
	</record>
</odoo>
