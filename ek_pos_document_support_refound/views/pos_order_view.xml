<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Inherit Form View to Modify it -->
        <record id="pos_order_document_support_refound_form" model="ir.ui.view">
            <field name="name">pos.order.refound</field>
            <field name="model">pos.order</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='fiscal_position_id']" position="after">
                    <!-- Add your fields or attributes here -->
                    <field name="l10n_latam_document_sustento_id" options="{'no_create': True}" attrs="{'invisible':[('l10n_latam_document_sustento_id', '=', False)]}"/>
                </xpath>



            </field>
        </record>

    </data>
</odoo>